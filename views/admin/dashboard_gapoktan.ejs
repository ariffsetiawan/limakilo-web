    <div id="" class="text-center" style="margin-top:100px;">
        <div class="container">

        <div class="row">
          <div class="col-md-12">
              <br>
              <a href="/commodity_add"><button type="button" class="btn btn-primary pull-right">Tambah Data</button></a>
              <br>
          </div>
        </div>


        <div class="row" style="min-height:400px;">
          <div class="col-md-12">
            <div class="page-header">
  
              <h2><%=title%></h2>
              
              
            </div>
            <div>
            <div class="panel">

            <table class="table table-condensed">
              <thead>
                <tr>
                  <th>Nama Gapoktan</th>
                  <th>Alamat</th>
                  <th>Kota</th>
                  
                  <th>Jumlah Komoditas</th>
                  <th>Status</th>
                  <th>Harga</th>
                </tr>
              </thead>
              <tbody>

              <% if(data.length){ 
                                      
                  for(var i = 0;i < data.length;i++) { %>
                <tr>
                  <th scope="row"><%=data[i].user_firstname %>&nbsp;<%=data[i].user_lastname %></th>
                  <td><%=data[i].user_address%></td>
                  <td><%=data[i].user_city%></td>
                  
                  <td><%=data[i].gc_quantity%> kg</td>
                  <td><%=data[i].gc_status%></td>
                  <td><%=numeral(data[i].gc_price).format('0,0')%></td>
                </tr>

                <% }

                 }else{ %>
                  <tr>
                  <td colspan="7">No Data.</td>
                  </tr>
                <% } %>


              </tbody>
            </table>
                    
            </div>
              
            </div>
          </div>
        </div>
        </div>
    </div>

   
   
