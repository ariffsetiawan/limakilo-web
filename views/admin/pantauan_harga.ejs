    <script type="text/javascript">
      $(function () {
          // $.getJSON('http://www.highcharts.com/samples/data/jsonp.php?a=e&filename=aapl-ohlc.json', function (data) {
          $.getJSON('chart.json', function (data) {

              // create the chart
              $('#chart').highcharts('StockChart', {

                plotOptions: {
                       candlestick: {
                          color: '#E64217',
                          upColor: '#1FC700'
                      }
                  },


                  rangeSelector : {
                      selected : 1
                  },

                  title : {
                      text : 'Harga Bawang'
                  },

                  series : [{
                      type : 'candlestick',
                      name : 'Harga Bawang',
                      data : data,
                      dataGrouping : {
                          units : [
                              [
                                  'week', // unit name
                                  [1] // allowed multiples
                              ], [
                                  'month',
                                  [1, 2, 3, 4, 6]
                              ]
                          ]
                      }
                  }]
              });
          });
      });

    </script>


